<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite [
        <!ELEMENT suite (test)*>
        <!ATTLIST suite
                thread-count CDATA #REQUIRED
                name CDATA #REQUIRED
                parallel CDATA #REQUIRED>
        <!ELEMENT test (parameter|classes)*>
        <!ATTLIST test
                name CDATA #REQUIRED>
        <!ELEMENT parameter (#PCDATA)>
        <!ATTLIST parameter
                name CDATA #REQUIRED
                value CDATA #REQUIRED>
        <!ELEMENT classes (class)*>
        <!ELEMENT class (#PCDATA)>
        <!ATTLIST class
                name CDATA #REQUIRED>
        ]>
<suite thread-count="50" name="DesktopBrowsers" parallel="tests">


