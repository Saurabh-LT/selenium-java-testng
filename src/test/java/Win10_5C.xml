<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite [
        <!ELEMENT suite (test)*>
        <!ATTLIST suite
                thread-count CDATA #REQUIRED
                name CDATA #REQUIRED
                parallel CDATA #REQUIRED>
        <!ELEMENT test (parameter|classes)*>
        <!ATTLIST test
                name CDATA #REQUIRED>
        <!ELEMENT parameter (#PCDATA)>
        <!ATTLIST parameter
                name CDATA #REQUIRED
                value CDATA #REQUIRED>
        <!ELEMENT classes (class)*>
        <!ELEMENT class (#PCDATA)>
        <!ATTLIST class
                name CDATA #REQUIRED>
        ]>
<suite thread-count="50" name="DesktopBrowsers" parallel="tests">


    <test name="1">
        <parameter name="browser" value="Chrome"/>
        <parameter name="version" value="latest"/>
        <parameter name="platform" value="win10"/>
        <parameter name="fixedIp" value="10.82.4.230"/>
        <parameter name="resolution" value="1920x1080"/>
        <parameter name="timezone" value="UTC+04:00"/>
        <parameter name="geoLocation" value="IN"/>
        <parameter name="tunnelName" value="prateekSignh"/>
        <parameter name="tunnel" value="false"/>
        <classes>
            <class name="magicleapTesting.magicLeap"/>

